<%- include('./layouts/Header', { pageTitle }) %>
<div class="home-videos">
    <% videos.forEach(function(item){ %>
        <%- include('./mixins/videoBlock', {
            video:{
                id:item._id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl
            }
        }) %>
    <% }); %>
</div>
<%- include('./layouts/Footer') %>
<div class="videoPlayer" id="jsVideoPlayer">
  <video src=<%=video.src%>></video>
  <div class="videoPlayer_controls">
    <div class="videoPlayer_column">
      <input class="videoPlayer_volume" id="jsVolume" type="range" min="0" max="1" step="0.1" value="0.5">
      <span id="jsVolumeBtn">
        <i class="fas fa-volume-down"></i>
      </span>
      <span id="currentTime">00:00:00</span>/<span id="totalTime">00:00:00</span>
    </div>
    <div class="videoPlayer_column">
      <span id="jsPlayButton">
        <i class="fas fa-play"></i>
      </span>
    </div>
    <div class="videoPlayer_column">
      <span id="jsFullScreen">
        <i class="fas fa-fa-expand"></i>
      </span>
    </div>         
  </div>
</div>

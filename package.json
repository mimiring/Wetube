{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "main": "index.js",
  "author": "Mimiring",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.8.0",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.2",
    "@babel/runtime": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "aws-sdk": "^2.624.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.9.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-flash-messages": "^0.1.1",
    "express-session": "^1.17.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ngrok": "^3.2.7",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore 'scss'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore src/assets,src/static,src/webpack.config.js",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "localtunnel": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4"
  }
}
